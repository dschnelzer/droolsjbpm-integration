			<xsd:element name="ACORDRq" type="tns:ACORDRq" />
			<xsd:complexType name="ACORDRq">
				<xsd:sequence>
					<xsd:element name="SignonRq" type="acord:SignonRq_Type" />
					<xsd:element name="InsuranceSvcRq"
						type="acord:InsuranceSvcRq_Type" />
					<!--  
					<xsd:element name="SignonRq" type="acord:SignonRq_Type" />
