			</xsd:complexType>


			<xsd:element name="ACORDRs" type="tns:ACORDRs" />
			<xsd:complexType name="ACORDRs">
				<xsd:sequence>
					<xsd:element name="SignonRs" type="acord:SignonRs_Type" />
					<xsd:element name="InsuranceSvcRs"
						type="acord:InsuranceSvcRs_Type" />
					<!--  
					<xsd:element name="SignonRs" type="acord:SignonRq_Type" />
					<xsd:element name="InsuranceSvcRs"
						type="acord:InsuranceSvcRs_Type" />
					-->
				</xsd:sequence>
			</xsd:complexType>
			
		</xsd:schema>





	</wsdl:types>





	<wsdl:message name="quote">
		<wsdl:part element="tns:ACORDRq" name="parameters">
		</wsdl:part>
	</wsdl:message>

	<wsdl:message name="quoteResponse">
		<wsdl:part element="tns:ACORDRs" name="parameters">
		</wsdl:part>
	</wsdl:message>

