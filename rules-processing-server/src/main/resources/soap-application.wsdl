	xmlns:tns="http://app.mcarta.com/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<wsdl:types>
		<xsd:schema attributeFormDefault="unqualified"
			elementFormDefault="qualified" targetNamespace="http://app.mcarta.com/"
			xmlns:tns="http://app.mcarta.com/" 
				xmlns:acord="http://www.ACORD.org/standards/PC_Surety/ACORD1/xml/"
			xmlns:xsd="http://www.w3.org/2001/XMLSchema">

			<xsd:import namespace="http://www.ACORD.org/standards/PC_Surety/ACORD1/xml/"
